{
  "name": "isomorphic-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "Isomorphic React Webpack Boilerplate",
  "main": "app/server.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "start": "NODE_ENV=production node build/server/app",
    "dev": "npm run prebuild && parallelshell 'npm run webpack:watch' 'npm run server:watch'",
    "webpack:watch": "webpack-dev-server --hot --inline",
    "webpack:build": "NODE_ENV=production webpack",
    "prebuild": "webpack -s",
    "server:watch": "nodemon --delay 2 _tmp/server/app.js",
    "cleanup": "rm -rf ./build ./public/app.js ./_tmp",
    "build": "npm run cleanup && npm run webpack:build"
  },
  "author": "Martin Broder <hello@martinbroder.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.3",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "react": "^0.13.3",
    "react-redux": "^2.1.0",
    "react-router": "^0.13.3",
    "redux": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-runtime": "^5.8.20",
    "eslint": "^1.3.1",
    "eslint-plugin-react": "^3.3.1",
    "jest-cli": "^0.5.1",
    "json-loader": "^0.5.2",
    "nodemon": "^1.4.1",
    "parallelshell": "^2.0.0",
    "react-a11y": "^0.2.6",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.1",
    "webpack-dev-server": "^1.10.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
