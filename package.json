{
  "name": "isomorphic-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "Isomorphic React Webpack Boilerplate",
  "main": "build/server/server.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src webpack config scripts",
    "coverage": "jest --coverage",
    "start": "NODE_ENV=production node build/server",
    "webpack:build": "npm run webpack:build:client && npm run webpack:build:server",
    "webpack:build:client": "NODE_ENV=production webpack --config webpack/webpack.client.config.js",
    "webpack:build:server": "NODE_ENV=production TARGET=SERVER webpack --config webpack/webpack.server.config.js",
    "copy": "mkdir -p ./build/public && cp -fR ./src/public/* ./build/public",
    "cleanup": "rm -rf ./build",
    "build": "npm run cleanup && npm run copy && npm run webpack:build",
    "dev": "node scripts/run start"
  },
  "author": "Martin Broder <hello@martinbroder.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.0",
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "redux": "^3.3.1",
    "redux-actions": "^0.9.1",
    "redux-loop": "^2.0.0",
    "redux-promise-middleware": "^2.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.4",
    "babel-core": "^6.7.4",
    "babel-eslint": "^5.0.0",
    "babel-jest": "^9.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015-webpack": "^6.4.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.11.2",
    "css-loader": "^0.23.1",
    "eslint": "^2.4.0",
    "eslint-plugin-react": "^4.2.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest-cli": "^0.9.2",
    "kneden": "^1.0.2",
    "lodash.merge": "^4.3.2",
    "postcss-loader": "^0.8.2",
    "react-a11y": "^0.3.2",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.13.0",
    "webpack": "^2.1.0-beta.4",
    "webpack-hot-middleware": "^2.10.0",
    "webpack-middleware": "^1.5.1",
    "webpack-stats-plugin": "^0.1.1"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
