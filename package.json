{
  "name": "universal-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "Universal React Webpack Boilerplate",
  "main": "build/server/server.js",
  "scripts": {
    "test": "echo 'No Tests'",
    "lint": "eslint src webpack config scripts",
    "start": "NODE_ENV=production node build/server",
    "webpack:build": "npm run webpack:build:client && npm run webpack:build:server",
    "webpack:build:client": "NODE_ENV=production webpack --config webpack/webpack.client.config.js",
    "webpack:build:server": "NODE_ENV=production TARGET=SERVER webpack --config webpack/webpack.server.config.js",
    "copy": "mkdir -p ./build/public && cp -fR ./src/public/* ./build/public",
    "cleanup": "rm -rf ./build",
    "build": "npm run cleanup && npm run copy && npm run webpack:build",
    "dev": "node scripts/run start"
  },
  "author": "Martin Broder <hello@martinbroder.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-promise-middleware": "^4.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-syntax-trailing-function-commas": "^6.13.0",
    "babel-plugin-transform-class-constructor-call": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-plugin-transform-export-extensions": "^6.8.0",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "browser-sync": "^2.15.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.4.0",
    "eslint-plugin-react": "^6.2.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.3",
    "kneden": "^1.0.5",
    "lodash.merge": "^4.6.0",
    "postcss-loader": "^0.11.1",
    "react-a11y": "^0.3.3",
    "react-hot-loader": "^3.0.0-beta.3",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.21",
    "webpack-hot-middleware": "^2.12.2",
    "webpack-middleware": "^1.5.1",
    "webpack-stats-plugin": "^0.1.3"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
