{
  "name": "isomorphic-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "Isomorphic React Webpack Boilerplate",
  "main": "build/server/server.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint src webpack config scripts",
    "coverage": "jest --coverage",
    "start": "NODE_ENV=production node build/server",
    "webpack:build": "npm run webpack:build:client && npm run webpack:build:server",
    "webpack:build:client": "NODE_ENV=production webpack --config webpack/webpack.client.config.js",
    "webpack:build:server": "NODE_ENV=production TARGET=SERVER webpack --config webpack/webpack.server.config.js",
    "copy": "mkdir -p ./build/public && cp -fR ./src/public/* ./build/public",
    "cleanup": "rm -rf ./build",
    "build": "npm run cleanup && npm run copy && npm run webpack:build",
    "dev": "node scripts/run start"
  },
  "author": "Martin Broder <hello@martinbroder.com>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.15.2",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.2",
    "redux-actions": "^0.10.0",
    "redux-promise-middleware": "^3.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^13.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-preset-es2015-webpack": "^6.4.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "browser-sync": "^2.13.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.13.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "jest-cli": "^13.0.0",
    "kneden": "^1.0.5",
    "lodash.merge": "^4.4.0",
    "postcss-loader": "^0.9.1",
    "react-a11y": "^0.3.3",
    "react-transform-hmr": "^1.0.4",
    "style-loader": "^0.13.1",
    "webpack": "^2.1.0-beta.14",
    "webpack-hot-middleware": "^2.11.0",
    "webpack-middleware": "^1.5.1",
    "webpack-stats-plugin": "^0.1.1"
  },
  "engines": {
    "node": ">=5.0.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
