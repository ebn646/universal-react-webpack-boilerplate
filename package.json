{
  "name": "isomorphic-react-webpack-boilerplate",
  "version": "0.0.1",
  "description": "Isomorphic React Webpack Boilerplate",
  "main": "app/server.js",
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "start": "node build/server/app",
    "dev": "npm run prebuild && parallelshell 'npm run webpack:watch:client' 'npm run webpack:watch:server' 'npm run server:watch'",
    "webpack:watch:server": "webpack --watch --quiet --no-info --config webpack/webpack.server.config.js",
    "webpack:watch:client": "NODE_ENV=webpack webpack-dev-server --hot --inline --config webpack/webpack.client.config.js",
    "webpack:build": "NODE_ENV=production webpack --config webpack",
    "prebuild": "webpack --config webpack -s",
    "server:watch": "node_modules/supervisor/lib/cli-wrapper.js -w _tmp/server -n exit _tmp/server/app.js",
    "cleanup": "rm -rf ./build ./public/app.js ./_tmp",
    "build": "npm run cleanup && npm run webpack:build"
  },
  "author": "Martin Broder <hello@martinbroder.com>",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.3",
    "immutable": "^3.7.5",
    "lodash": "^3.10.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-redux": "^3.1.0",
    "react-router": "^1.0.0-rc3",
    "redux": "^3.0.2"
  },
  "devDependencies": {
    "babel-core": "^5.8.25",
    "babel-eslint": "^4.1.3",
    "babel-jest": "^5.3.0",
    "babel-loader": "^5.3.2",
    "babel-plugin-react-transform": "^1.1.1",
    "babel-runtime": "^5.8.25",
    "eslint": "^1.6.0",
    "eslint-plugin-react": "^3.5.1",
    "jest-cli": "^0.5.10",
    "json-loader": "^0.5.3",
    "parallelshell": "^2.0.0",
    "react-a11y": "^0.2.6",
    "react-transform-hmr": "^1.0.1",
    "supervisor": "^0.9.1",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/"
    ]
  }
}
